void setup() {
  size(600, 400);
}

void draw() {
  background(153, 204, 255);

  int centerX = width / 2;
  int centerY = height / 2;


  int diameter = 200;

  fill(280, 0, 0); 
  ellipse(centerX, centerY, diameter, diameter);

  fill(255); 
  ellipse(centerX, centerY, diameter / 2, diameter / 2);


  fill(65, 60, 222); 
  rect(centerX - diameter / 2, centerY - 10, diameter, 20);
}

